mixin header(obj)
    header(class="header")
        div(class="box")
            div(class="grid-container fluid")
                div(class="grid-x")
                    div(class="large-3 medium-2 columns")
                        include ../nav/nav.pug
                        +nav(["Home", "About"])

                    div(class="large-6 medium-8 columns")
                        include ../../logo/logo.pug
                        +logo()

                    div(class="large-3 medium-12 columns header__search")
                        include ../search/search.pug
                        +search()
    !="\n"

    // END .header
